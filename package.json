    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.2",
    "electron": "^12.0.7",
    "electron-builder": "^22.10.5",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
