{
  "name": "mel-client-13test",
  "version": "0.1.0",
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:build": "ng build && ncp ./package.json ./dist/package.json",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "--": "// main-process",
    "electron:serve-tsc": "wait-on tcp:3041 && tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:3041 && npm run electron:serve-tsc && npx electron . --serve=http://localhost:3041/#",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lerchenreich/mel-client.git"
  },
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~13.0.3",
    "@angular/localize": "~13.0.3",
    "@angular/material": "^12.2.13",
    "@angular/material-moment-adapter": "^12.2.13",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "~13.0.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@ngneat/until-destroy": "^8.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@popperjs/core": "^2.5.4",
    "@types/jquery": "^3.5.1",
    "angular-bootstrap-md": "^12.1.0",
    "animate.css": "^4.1.1",
    "class-validator": "^0.13.2",
    "electron-reload": "^1.5.0",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "jszip": "^3.6.0",
