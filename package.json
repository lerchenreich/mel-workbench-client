{
  "name": "workbench-client",
  "version": "0.1.2",
  "main": "main.js",
  "private": true,
  "scripts": {
    "-": "// Renderer",
    "upd": "npm un mel-common mel-client mel-workbench-api && npm i mel-common mel-client mel-workbench-api",
    "ngServe": "ng serve",
    "ngBuild": "ng build && npm run cpAll",
    "cpPackage": "ncp ./package.json ./dist/package.json",
    "cpMeli18n": "ncp ./node_modules/mel-client/assets/i18n/ ./src/assets/i18n/mel/",
    "cpMelIcons": "ncp ./node_modules/mel-client/assets/icons/ ./src/assets/icons/",
    "cpMelImages": "ncp ./node_modules/mel-client/assets/images/ ./src/assets/images/",
    "cpMelStyles": "ncp ./node_modules/mel-client/assets/styles/ ./src/assets/styles/",
    "cpAll": "npm run cpPackage && npm run cpMeli18n && npm run cpMelIcons && npm run cpMelImages && npm run cpMelStyles",
    "--": "// Renderer- and electron building",
    "build": "ng build --base-href ./ && npm run e:build",
    "postbuild": "npm run cpAll",
    "build:dev": "npm run build -- -c dev && npm run e:build",
    "build:prod": "npm run build -- -c production && npm run e:build",
    "---": "// main-process",
    "e:build": "tsc -p tsconfig.serve.json",
    "e:serve": "wait-on tcp:3041 && npx electron . --serve=http://localhost:3041/#",
    "e:local": "npm run build:prod && npx electron .",
    "----": "// ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lerchenreich/mel-client-13test.git"
  },
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/localize": "~13.0.3",
    "@angular/material": "^13.0.3",
    "@angular/material-moment-adapter": "^13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "~13.0.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@ngneat/until-destroy": "^8.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@popperjs/core": "^2.5.4",
    "@types/jquery": "^3.5.1",
    "angular-bootstrap-md": "^12.2.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "class-validator": "^0.13.2",
    "electron-reload": "^1.5.0",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "jszip": "^3.6.0",
    "mdbootstrap": "^4.19.2",
    "mel-client": "^0.0.143",
    "mel-common": "^1.1.38",
    "mel-workbench-api": "^1.0.15",
    "moment": "^2.28.0",
    "ngx-electron": "^2.2.0",
    "ngx-scrollbar": "^7.4.2",
    "ngx-translate-multi-http-loader": "^7.0.1",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.4",
    "@angular/cli": "^13.0.4",
    "@angular/compiler-cli": "^13.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.14.35",
    "@types/validator": "^13.7.0",
    "codelyzer": "^6.0.2",
    "electron": "^12.0.7",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslib": "^2.2.0",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5",
    "wait-on": "^5.3.0"
  }
}
